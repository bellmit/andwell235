<tableList describe="库房管理库存查询统计" xmlName="KFGL_KCCXTJ">
  <table>
    <tableInfo>
      <caption>库存查询统计</caption>
      <tableName>T_CK_CK</tableName>
      <sql>SELECT A.JX,T_JH_JX.MC JXMC,JH,T_JH.MC JHMC,T_XT_JLDW.MC JLDW,A.ZSL,ZL,T_JH_ZL.MC ZLMC,HCLB,T_XT_HCLB.MC HCMC,A.ZJ,A.ZJ/ZSL PJJ FROM (
select  JX,JH,SUM(T_CK_CK.SL)ZSL,ZL,HCLB,SUM(DJ*SL*HL)ZJ from  T_CK_CK WHERE 1=1 AND SL&gt;0
GROUP BY JX,JH,ZL,HCLB  ORDER BY JX,JH,HCLB,ZL 
)A 
LEFT JOIN T_JH_JX ON T_JH_JX.ID=A.JX
LEFT JOIN T_JH ON T_JH.ID=A.JH AND T_JH.JX=A.JX
LEFT JOIN T_XT_JLDW ON T_XT_JLDW.ID=T_JH.DW
LEFT JOIN T_JH_ZL ON T_JH_ZL.ID=A.ZL
LEFT JOIN T_XT_HCLB ON T_XT_HCLB.ID=A.HCLB</sql>
    </tableInfo>
