      <tableName>RKTXDetail2</tableName>
      <sql>SELECT  T_JH_ZL.MC as ZLMC,T_JH_JX.MC as JXMC,T_XT_BZLB.MC as BZMC,     T_CK_CRKQD.*,T_CK_DX.GZRQ,T_CK_DX.FXR,T_CK_DX.PGR,T_CK_DX.GZXX,T_CK_DX.ZJSJ,T_CK_DX.ZJDW,T_CK_DX.ZJR,T_JH.MC JHMC,T_XT_JLDW.MC JLDW,T_CK_Dx.xljb,    T_CK_DX.Cjsj,T_CK_DX.Cjdw,T_CK_DX.CXDW,T_CK_DX.Cjr,T_CK_DX.YJ,T_CK_DX.htbh,T_CK_DX.SYDW,T_CK_DX.ZY,T_CK_DX.ZXSL,T_CK_DX.yJ,T_CK_DX.BH As DXBH,T_XT_HCLB.mc as HCLBMC               FROM T_CK_DX           left join T_CK_CRKQD on  T_CK_DX.BH=T_CK_CRKQD.Bh and T_CK_CRKQD.JH=T_CK_DX.Jh and T_CK_CRKQD.XH=T_CK_DX.Xh           left join T_JH_ZL on T_JH_ZL.ID=T_CK_CRKQD.ZL   left join T_JH_JX on T_JH_JX.ID=T_CK_CRKQD.JX left join T_XT_HCLB on T_XT_HCLB.id=T_CK_CRKQD.Hclb   LEFT JOIN T_JH ON T_JH.ID=T_CK_DX.JH LEFT JOIN T_XT_JLDW ON T_XT_JLDW.ID=T_JH.DW  left join T_XT_BZLB on T_XT_BZLB.id=T_CK_CRKQD.BBZ          WHERE 1=1 </sql>
