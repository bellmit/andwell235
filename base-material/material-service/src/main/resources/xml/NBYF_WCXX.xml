  <table>
    <tableInfo>
      <caption>内部油封完成查询</caption>
      <tableName>T_CK_YFK</tableName>
      <sql>select BH,DWID,T_XT_DW.JC LHDW,T_CK_YFK.JX,T_JH_JX.MC JXMC,JH,XH,T_CK_YFK.SL,ZL,DJ,ZJ,CCRQ,YFRQ,YFQX,T_XT_JLDW.MC JLDW,T_JH_JH.MC JHMC,JWH,CKDM,T_JH_ZL.MC ZLMC,ffdw,
CASE WHEN XZ=0 THEN &apos;油封数据待确认&apos;   WHEN XZ=1 THEN &apos;油封完成&apos;   WHEN XZ=2 THEN &apos;油封完成待接收确认&apos;  WHEN XZ=3 THEN &apos;油封完成待上架&apos; WHEN XZ=4 THEN &apos;已上架&apos; WHEN XZ=5 THEN &apos;外借油封&apos; ELSE &apos;&apos; END ZT,XZ,RKRQ
from T_CK_YFK
LEFT JOIN T_JH_JH ON T_JH_JH.ID=T_CK_YFK.JH
LEFT JOIN T_XT_JLDW ON T_XT_JLDW.ID=T_JH_JH.DW
LEFT JOIN T_JH_JX ON T_JH_JX.ID=T_CK_YFK.JX 
LEFT JOIN T_JH_ZL ON T_JH_ZL.ID=T_CK_YFK.ZL 
LEFT JOIN T_XT_DW ON T_XT_DW.ID=T_CK_YFK.FFDW
WHERE 1=1  ORDER BY XZ </sql>
