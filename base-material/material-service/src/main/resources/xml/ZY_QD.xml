  <table>
    <tableInfo>
      <caption>库存信息</caption>
      <tableName>T_CK_CK</tableName>
      <sql>select A.DWID,T_QX_DW.MC DWMC,A.JX,T_JH_JX.MC JXMC,A.JH,T_JH.MC JHMC,T_XT_JLDW.MC JLDW,A.ZL,T_JH_ZL.MC ZLMC,sum(A.Sl) SL 
from T_CK_CK A
left join T_QX_DW on A.DWID=T_QX_DW.ID
left join T_JH_JX on A.JX=T_JH_JX.ID
left join T_JH  on A.JH=T_JH.ID and T_JH.JX=A.JX
left join T_XT_JLDW on T_JH.Dw=T_XT_JLDW.ID
left join T_JH_ZL on A.ZL=T_JH_ZL.Id
left join T_XT_BZLB on A.BBZ=T_XT_BZLB.ID
WHERE 1=1 
group by A.DWID,T_QX_DW.MC ,A.JX,T_JH_JX.MC ,A.JH,T_JH.MC ,T_XT_JLDW.MC,A.ZL,T_JH_ZL.MC  
ORDER BY A.DWID DESC</sql>
