      <caption>待修航材价位管理</caption>
      <tableName>T_CK_DX</tableName>
      <sql>select T_CK_DX.BH,T_JH_JX.MC as JXMC,JH,T_JH.MC,XH,T_CK_DX.SL,to_char(CCRQ,&apos;YYYY-MM-DD&apos;) CCRQ,
to_char(YSRQ,&apos;YYYY-MM-DD&apos;) YSRQ,ZSMXS-nvl(ZFXXS,0) SYXS,RKRQ, to_char(YFRQ,&apos;YYYY-MM-DD&apos;)YFRQ,
TO_Char(ADD_MONTHS(YFRQ,12*YFQX),&apos;YYYY-MM-DD&apos;)YFQZ, T_JH_ZL.mc ZLMC,T_QX_DW.JM CKDM,hclb,T_XT_HCLB.MC HCLBMC,
T_CK_DX.BZ,T_CK_DX.JX,T_CK_DX.JWH,zl,dwid,xz
from T_CK_DX 
left join T_JH on T_JH.ID=JH AND T_JH.JX=T_CK_DX.JX
left join T_JH_JX on T_JH_JX.id=T_CK_DX.jx 
left join T_JH_ZL on T_JH_ZL.id=zl 
left join T_QX_DW on T_QX_DW.ID=CKDM
left join T_XT_HCLB ON T_XT_HCLB.ID=T_CK_DX.HCLB
where 1=1 AND T_CK_DX.SL&gt;0</sql>
    </tableInfo>
