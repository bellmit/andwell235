      <caption>用户信息表</caption>
      <tableName>t_qx_yh</tableName>
      <sql>SELECT DWID,CK.JX,JXMC,CK.JH,SL,DWMC,JHMC,JLDWMC, nvl(DCSL,0) DCSL, SL-nvl(DCSL,0) SYSL,nvl(PJDJ,0)PJDJ,nvl(ZJ,0) ZJ,ZLMC,CK.ZL FROM (
SELECT A.DWID,A.JX,A.JH,SUM(A.SL) SL,SUM(A.SL*A.HL*A.DJ)/SUM(A.SL) PJDJ,SUM(A.SL*A.HL*A.DJ)ZJ,T_QX_DW.MC DWMC,T_JH.MC JHMC,T_JH_JX.MC JXMC,T_XT_JLDW.MC JLDWMC,ZL,T_JH_ZL.MC ZLMC 
FROM T_CK_CK A
LEFT JOIN T_QX_DW ON A.DWID=T_QX_DW.ID 
LEFT JOIN T_JH ON A.JH=T_JH.ID AND T_JH.JX=A.JX
LEFT JOIN T_JH_JX ON A.JX=T_JH_JX.ID 
LEFT JOIN T_XT_JLDW ON T_JH.DW=T_XT_JLDW.ID
LEFT JOIN T_JH_ZL ON T_JH_ZL.ID=ZL
WHERE 1=1 AND A.SL&gt;0
GROUP BY A.DWID,A.JX,A.JH,T_QX_DW.MC,T_JH.MC,T_JH_JX.MC,T_XT_JLDW.MC,ZL,T_JH_ZL.MC)CK
LEFT JOIN(
SELECT T_DB_DBQD.JX,JH, nvl(SUM(SL),0) DCSL,ZL  FROM T_DB_DBQD 
 left join T_DB_DB on T_DB_DB.DBDH=T_DB_DBQD.DBDH  where ZT = 6 GROUP BY T_DB_DBQD.JX,JH,FFDW,ZL   
)DC
ON CK.JX=DC.JX AND CK.JH=DC.JH AND CK.ZL=DC.ZL ORDER BY CK.JX,CK.JH</sql>
    </tableInfo>
