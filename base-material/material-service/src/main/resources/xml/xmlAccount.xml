      <caption>日历期预警</caption>
      <tableName>T_CK_CK</tableName>
      <sql>SELECT A.JX,T_JH_JX.MC JXMC,JH,T_JH.MC JHMC,T_XT_JLDW.MC JLDW,XH,A.SL,ZL,T_JH_ZL.MC ZLMC,DJ,ZJ,A.DW,JWH,CKDM,HCLB,BBZ,A.DWID,
T_XT_HCLB.MC HCLBMC,CCRQ,FXRQ,ZRLQ,SFRLQ,YFRQ,YFQX,XFRQ,case when XFRQ IS null then add_months(CCRQ,ZRLQ*12)  WHEN XFRQ IS NOT null   then add_months(XFRQ,ZRLQ*12) END RLQZ,ADD_MONTHS(YFRQ,12*YFQX) YFQZ
FROM T_CK_CK A
LEFT JOIN T_JH_JX ON T_JH_JX.ID=A.JX
LEFT JOIN T_JH ON T_JH.ID=A.JH AND T_JH.JX=A.JX
LEFT JOIN T_XT_JLDW ON T_JH.DW=T_XT_JLDW.ID
LEFT JOIN T_XT_HCLB ON T_XT_HCLB.ID=A.HCLB
LEFT JOIN T_JH_ZL ON T_JH_ZL.ID=A.ZL 
WHERE  ZRLQ IS NOT NULL AND A.SL&gt;0 AND ZRLQ&gt;0
</sql>
