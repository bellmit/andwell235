  <table>
    <tableInfo>
      <caption>保管队油封数据查询</caption>
      <tableName>T_CK_YFK</tableName>
      <sql>select BH,DWID,T_CK_YFK.JX,T_JH_JX.MC JXMC,JH,XH,T_CK_YFK.SL,ZL,DJ,ZJ,TO_CHAR(CCRQ,&apos;yyyy-mm-dd&apos;) CCRQ,TO_CHAR(YFRQ,&apos;yyyy-mm-dd&apos;) YFRQ,YFQX,T_XT_JLDW.MC JLDW,T_JH.MC JHMC,JWH,CKDM,T_JH_ZL.MC ZLMC,
CASE WHEN XZ=0 THEN &apos;油封数据待确认&apos;   WHEN XZ=1 THEN &apos;油封完成&apos;   WHEN XZ=2 THEN &apos;油封完成待接收确认&apos;  WHEN XZ=3 THEN &apos;油封完成待上架&apos; WHEN XZ=4 THEN &apos;已上架&apos; WHEN XZ=5 THEN &apos;外接油封&apos; ELSE &apos;其他状态&apos; END ZT,RKRQ
from T_CK_YFK LEFT JOIN T_JH ON T_JH.ID=T_CK_YFK.JH AND T_JH.JX=T_CK_YFK.JX
LEFT JOIN T_XT_JLDW ON T_XT_JLDW.ID=T_JH.DW
LEFT JOIN T_JH_JX ON T_JH_JX.ID=T_CK_YFK.JX 
LEFT JOIN T_JH_ZL ON T_JH_ZL.ID=T_CK_YFK.ZL WHERE 1=1</sql>
