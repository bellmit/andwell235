<tableList describe="油封期预警航材" xmlName="YJ_YFQYJ">
  <table>
    <tableInfo>
      <caption>油封期预警</caption>
      <tableName>T_CK_CK</tableName>
      <sql>SELECT B.JX,B.JXMC,JH,B.JHMC,B.JLDW,XH,B.SL,ZL,B.ZLMC,DJ,ZJ,B.DW,JWH,CKDM,HCLB,BBZ,
B.HCLBMC,CCRQ,ZRLQ,SFRLQ,YFRQ,YFQX,FXRQ,B.XFRQ,B.DQRLQ, B.YFQZ,B.DWID,B.qclbmc ,
  B.YJYF,B.EJYF,B.SJYF,B.YJYFQZ,B.EJYFQZ,B.SJYFQZ,
  CASE WHEN YFQZ&gt;SJYFQZ THEN &apos;0&apos; WHEN YFQZ&lt;SJYFQZ AND YFQZ&gt;EJYFQZ THEN &apos;3&apos; WHEN YFQZ&lt;EJYFQZ AND YFQZ&gt;YJYFQZ THEN &apos;2&apos; WHEN YFQZ&lt;YJYFQZ THEN &apos;1&apos; ELSE &apos;4&apos; END ZT FROM (
SELECT T_CK_CK.JX,T_JH_JX.MC JXMC,JH,T_JH.MC JHMC,T_CK_CK.DW JLDW,XH,T_CK_CK.SL,ZL,T_JH_ZL.MC ZLMC,DJ,ZJ,T_CK_CK.DW,JWH,CKDM,HCLB,BBZ,
T_XT_HCLB.MC HCLBMC,CCRQ,ZRLQ,SFRLQ,YFRQ,YFQX,FXRQ,XFRQ,ADD_MONTHS(CCRQ,12*ZRLQ) DQRLQ,ADD_MONTHS(YFRQ,12*YFQX) YFQZ,T_CK_CK.DWID,CASE WHEN QCLB=1 THEN &apos;周转航材&apos; 
WHEN QCLB=2 THEN &apos;战备器材&apos;else&apos;其他&apos; end qclbmc ,
  YJYF,EJYF,SJYF,SYSDATE+YJYF YJYFQZ,SYSDATE+EJYF EJYFQZ,SYSDATE+SJYF SJYFQZ
FROM T_CK_CK
LEFT JOIN T_JH_JX ON T_JH_JX.ID=T_CK_CK.JX
LEFT JOIN T_JH ON T_JH.ID=T_CK_CK.JH AND T_JH.JX=T_CK_CK.JX
LEFT JOIN T_XT_JLDW ON T_JH.DW=T_XT_JLDW.MC
LEFT JOIN T_XT_HCLB ON T_XT_HCLB.ID=T_CK_CK.HCLB
LEFT JOIN T_XT_HCYJ ON 1=1  
LEFT JOIN T_JH_ZL ON T_JH_ZL.ID=T_CK_CK.ZL  )B WHERE 1=1 AND B.SL&gt;0 and  B.YFRQ is not null 
</sql>
