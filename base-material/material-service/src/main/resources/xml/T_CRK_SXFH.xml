      <caption>送修返回入库</caption>
      <tableName>T_CRK_SXFH</tableName>
      <sql>Select T_CK_SX.HCLB,HTBH,t_Jh_Jx.MC as jxMC,T_CK_SX.JX,T_JH.mc as jhMC,T_CK_SX.JH,T_CK_SX.XH,T_CK_SX.sl,to_char(SXRQ,&apos;YYYY-MM-DD&apos;) SXRQ, T_CF.Mc as CXDWMC,htbh,JXQN,JXQXS,T_CK_SX.DXBH,T_CK_DX.CCRQ,T_CK_SX.CXDW      
                    from T_CK_SX      
                    LEFT JOIN T_JH_JX ON T_JH_JX.ID=T_CK_SX.JX       
                    LEFT JOIN T_CK_DX ON T_CK_DX.BH=T_CK_SX.DXBH AND T_CK_DX.JX=T_CK_SX.JX AND T_CK_DX.JH=T_CK_SX.JH AND T_CK_DX.XH=T_CK_SX.XH 
                    left join T_JH on T_JH.iD=T_CK_SX.JH  AND T_JH.JX=T_CK_SX.JX 
                left join T_CF on T_CF.ID=T_CK_SX.Cxdw where 1=1</sql>
