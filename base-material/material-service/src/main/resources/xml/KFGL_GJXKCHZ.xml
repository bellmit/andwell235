<tableList describe="库房管理各机型库存汇总" xmlName="KFGL_GJXKCHZ">
  <table>
    <tableInfo>
      <caption>各机型库存汇总统计</caption>
      <tableName>T_CK_CK</tableName>
      <sql>SELECT COUNT(*)XIC,HCLB,T_XT_HCLB.MC LBMC,JX,T_JH_JX.MC JXMC,SUM(ZS) ZS FROM (
SELECT JX,SUM(SL)ZS,JH,HCLB FROM T_CK_CK
WHERE 1=1 AND XZ=0
GROUP BY JX,JH,HCLB
)A
LEFT JOIN T_JH_JX ON T_JH_JX.ID=A.JX
LEFT JOIN T_XT_HCLB ON T_XT_HCLB.ID=HCLB
GROUP BY HCLB,T_XT_HCLB.MC,JX,T_JH_JX.MC order by JX</sql>
    </tableInfo>
