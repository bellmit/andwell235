      <tableName>T_CK_BFK</tableName>
      <sql>select T_JH_JX.MC as JXMC,
JH,T_JH.MC,XH,T_CK_BFK.SL,
RKRQ, T_JH_ZL.mc ZLMC,zl,
T_QX_DW.JM CKDM,hclb,T_XT_HCLB.MC HCLBMC,T_CK_BFK.BZ,T_CK_BFK.JX,T_CK_BFK.JWH,T_CK_BFK.xz,bh,dwid,V_DW.mc dwmc
from T_CK_BFK 
left join T_JH on T_JH.ID=JH 
left join T_JH_JX on T_JH_JX.id=T_CK_BFK.jx 
left join T_JH_ZL on T_JH_ZL.id=zl 
left join T_QX_DW on T_QX_DW.ID=CKDM
LEFT JOIN V_DW on T_CK_BFK.dwid = V_DW.id
left join T_XT_HCLB ON T_XT_HCLB.ID=T_CK_BFK.HCLB
where 1=1</sql>
