            <tableName>T_CK_CRKQD</tableName>
            <sql>select T_CK_FYFS.mc as YSMC, FFDWMC,
                JSDWMC,T_CK_CRKQD.Bh,T_CK_CRKQD.Cxsgin,T_CK_CRKQD.Fjh,T_CK_CRKQD.Jh,
                T_CK_CRKQD.Xh,T_CK_CRKQD.Jwh,T_CK_CRKQD.Dj,T_CK_CRKQD.Zj,T_CK_CRKQD.Hl,T_CK_CRKQD.Tzs,T_CK_CRKQD.Sl,T_CK_CRKQD.Sl
                as sll,
                T_CK_CRKQD.Sjs,T_CK_CRKQD.Jjs,T_CK_CRKQD.Ckdm,T_CK_CRKQD.Ccrq,T_CK_CRKQD.Xfrq,T_CK_CRKQD.Smzt,T_CK_CRKQD.Rkrq,
                T_CK_CRKQD.Zsmn,T_CK_CRKQD.Zsmxs,T_CK_CRKQD.Zsmxh,T_CK_CRKQD.Zsmql,T_CK_CRKQD.Zfxcs,T_CK_CRKQD.Yfrq,T_CK_CRKQD.Yfqx,
                T_CK_CRKQD.Fxrq,T_CK_CRKQD.Fxsmn,T_CK_CRKQD.Fxsmxs,T_CK_CRKQD.Fxcs,T_CK_CRKQD.Sbqxn,T_CK_CRKQD.sbqxxs,T_CK_CRKQD.Bz,
                T_CK_CRKQD.bczfxxs,T_CK_CRKQD.Bcfxxs,T_CK_CRKQD.Zxhcs,T_CK_CRKQD.Bczxhcs,T_CK_CRKQD.Bcxhcs,T_CK_CRKQD.Zfxxs,
                T_CK_CRKQD.Zqlcs,T_CK_CRKQD.Bczqlcs,T_CK_CRKQD.Bcqlcs,T_CK_CRKQD.Yfcs,T_CK_CRKQD.Zyfcs
                ,T_JH_JX.mc as JXMC,T_JH.mc as JHMC,T_JH_ZL.MC ZLMC,T_CK_CRK.WH,T_CK_CRK.YJ
                from T_CK_CRKQD
                left join T_CK_CRK on T_CK_CRK.BH=T_CK_CRKQD.BH
                left join T_JH_JX on T_JH_JX.ID=T_CK_CRKQD.Jx
                left join T_JH on T_JH.id=T_CK_CRKQD.jh AND T_JH.JX=T_CK_CRKQD.JX
                left join T_CK_FYFS on T_CK_FYFS.id=T_CK_CRK.Ysfs
                LEFT JOIN T_JH_ZL ON T_JH_ZL.ID=T_CK_CRKQD.ZL
                where 1=1 AND T_CK_CRKQD.CXSGIN=0 ORDER BY T_CK_CRKQD.JH,T_CK_CRKQD.XH,T_JH.MC
            </sql>
