    <table>
        <tableInfo>
            <caption>发付导出查询</caption>
            <tableName>T_CK_CRK</tableName>
            <sql>select '0' XZ,T_CK_CRK.BH,T_CK_CRK.Yj,T_CK_CRK.Wh,T_JH_JX.ID jx,ffdwmc,jsdwmc,
                A.Mc as FFMC,b.mc as JSMC, to_char(T_CK_CRK.RQ,'YYYY-MM-DD')RQ,T_JH_JX.mc as JXMC,
                T_CK_FYFS.Mc as FYFSMC
                from T_CK_CRK
                left join V_DW A on A.id=T_CK_CRK.Ffdw
                left join V_DW B on B.id=T_CK_CRK.Jsdw
                left join T_JH_JX on T_CK_CRK.Fjlx=T_JH_JX.Id
                left join T_CK_FYFS on T_CK_FYFS.ID=T_CK_CRK.YSFS
                where 1=1 AND T_CK_CRK.LB='2002' order by T_CK_CRK.WH
            </sql>
