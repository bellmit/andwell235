      <caption>旅团申请下发历史信息</caption>
      <tableName>T_DB_LTSQ_TEMP</tableName>
      <sql>select T_DB_LTSQ_TEMP.*,v1.mc sqdwmc,v2.mc jsdwmc,T_JH_JX.mc jxmc,
CASE WHEN clzt=&apos;1&apos; THEN &apos;未处理&apos; WHEN clzt=&apos;2&apos; THEN &apos;已处理&apos; ELSE &apos;未知&apos; END clztmc
from T_DB_LTSQ_TEMP
LEFT JOIN V_DW v1 ON T_DB_LTSQ_TEMP.sqdw = v1.id
LEFT JOIN V_DW v2 ON T_DB_LTSQ_TEMP.JSDW = v2.id
LEFT JOIN T_JH_JX ON T_JH_JX.id = T_DB_LTSQ_TEMP.jx
where 1=1 
</sql>
