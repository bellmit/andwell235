      <caption>库存信息</caption>
      <tableName>MASTER</tableName>
      <sql>SELECT  A.*,A.ckDm,A.XH,C.MC AS JXMC,A.JH,A.SL,A.XZ,To_char(A.CCRQ,&apos;YYYY-MM-dd&apos;) ccrq1,A.RKRQ,D.MC AS JHMC ,A.dwID  ,T_JH_ZL.Mc as ZLMC,T_XT_HCLB.MC HCLBMC,T_XT_JLDW.MC JLDW,ADD_MONTHS(A.YFRQ,12*A.YFQX) YFQZ,GLFS,JWH,A.RFID,A.YSID,CASE WHEN QCLB=1 THEN &apos;周转&apos;WHEN QCLB=2 THEN &apos;战备&apos; else &apos;其他&apos;end QCLBMC
	  FROM T_CK_CK A  LEFT JOIN T_JH_JX C ON A.JX=C.ID    
	  LEFT JOIN T_JH D ON D.ID = A.JH  AND D.JX=A.JX   left join T_XT_HCLB on T_XT_HCLB.id=A.Hclb    left join t_Jh_Zl on T_JH_ZL.id =A.Zl      LEFT JOIN T_XT_JLDW ON T_XT_JLDW.ID=D.DW    WHERE 1=1 and A.SL&gt;0 order by a.jh,ADD_MONTHS(A.YFRQ,12*A.YFQX),CCRQ1</sql>
