      <caption>件号关注</caption>
      <tableName>T_JH</tableName>
      <sql>SELECT T_JH.ID JH,T_JH.MC,T_XT_JLDW.MC DW,T_JH_JX.MC JX,T_JH.JX SJX,ZYLX,T_JH_ZYLX.MC LXMC,t_JH_jx.Id id FROM T_JH
LEFT JOIN t_JH_jx ON t_JH_jx.ID=T_JH.JX
LEFT JOIN T_XT_jldw on T_XT_jldw.id=T_JH.dw 
LEFT JOIN T_JH_ZYLX ON T_JH_ZYLX.ID=ZYLX
where 1=1 ORDER BY T_JH.JX
</sql>
