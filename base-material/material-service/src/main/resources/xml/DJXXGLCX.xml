      <caption>大件信息管理查询</caption>
      <tableName>T_CK_CK</tableName>
      <sql>select
                T_XT_DW.MC DWMC,JX,T_JH_JH.MC JHMC,JH,XH,SL,t_ck_ck.DW,DJ,ZJ,T_XT_BZLB.MC BZMC,t_ck_ck.HL YHL,ZL,JWH,ROUND(DJ*t_ck_ck.HL)YHLDJ,ROUND(ZJ*t_ck_ck.HL)YHLZJ,
                ROUND(DJ*T_XT_BZLB.HL) XHLDJ,(ZJ*T_XT_BZLB.HL)XHLZJ,JXQN,JXQXS,CCRQ,YSRQ,RKRQ,YFRQ,YFQX,T_XT_HCLB.MC HCLB,ZSMN,ZSMXS,SFRLQ,ZRLQ,ZFANXCS,FANXCS,SFQSM,SFQSYSM,
                SFQXS,ZGQCS,GQCS,BZ
                ,(case when QCLB=1 then &apos;周转器材&apos;  when QCLB=2 then &apos;战备器材&apos; end) as QCLB
                from t_ck_ck
                LEFT JOIN T_JH_JH ON t_ck_ck.Jh=T_JH_JH.ID
                LEFT JOIN T_XT_DW ON t_ck_ck.DWID=T_XT_DW.ID
                LEFT JOIN T_XT_BZLB ON t_ck_ck.BBZ=T_XT_BZLB.ID
                LEFT JOIN T_XT_HCLB ON t_ck_ck.HCLB=T_XT_HCLB.ID
                where
                (jx,jh) in(select jx,jh from t_jh_dj)</sql>
    </tableInfo>
