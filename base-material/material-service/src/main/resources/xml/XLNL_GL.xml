      <caption>机型机件列表</caption>
      <tableName>T_WX_NL</tableName>
      <sql>select A.ID,A.JX,A.JH,T_JH_JX.MC JXMC,T_JH_JH.MC JHMC,T_XT_JLDW.MC JLDW
        from T_WX_NL A
        LEFT JOIN T_JH_JX ON T_JH_JX.ID=A.JX
        LEFT JOIN T_JH_JH ON T_JH_JH.ID=A.JH
        LEFT JOIN T_XT_JLDW ON T_XT_JLDW.ID=T_JH_JH.DW
