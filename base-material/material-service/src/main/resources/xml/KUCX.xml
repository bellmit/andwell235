      <caption>库存查询</caption>
      <tableName>T_CK_CK_TEMP</tableName>
      <sql>select T_JH_JX.MC JX,JH,T_JH.MC JHMC,XH,T_CK_CK_TEMP.SL,to_char(CCRQ,'YYYY-MM-DD') CCRQ,to_char(YSRQ,'YYYY-MM-DD') YSRQ,ZSMXS-nvl(ZFXXS,0) SYXS,to_char(RKRQ,'YYYY-MM-DD')RKRQ,
        to_char(YFRQ,'YYYY-MM-DD')YFRQ,TO_Char(ADD_MONTHS(YFRQ,12*YFQX),'YYYY-MM-DD')YFQZ, T_JH_ZL.mc ZL,T_QX_DW.JM CKDM,hclb,T_XT_HCLB.MC HCLBMC,T_CK_CK_TEMP.DJ,T_CK_CK_TEMP.DJ*T_CK_CK_TEMP.SL ZJ,T_CK_CK_TEMP.HL,T_XT_BZLB.HL NEWHL,BBZ,T_XT_BZLB.MC BZMC,T_CK_CK_TEMP.BZ,
        ZSMN,ZSMXS,SFRLQ,ZRLQ,ZFANXCS,FANXCS,XFRQ,SFQSM,SFQSYSM,ZGQCS,GQCS,SFQXS,JXQN,JXQXS,DCRQ
        from T_CK_CK_TEMP
        left join T_JH on T_JH.ID=JH and T_JH.JX=T_CK_CK_TEMP.JX
        left join T_JH_JX on T_JH_JX.id=T_CK_CK_TEMP.jx
        left join T_JH_ZL on T_JH_ZL.id=zl
        left join T_QX_DW on T_QX_DW.ID=CKDM
        left join T_XT_HCLB ON T_XT_HCLB.ID=T_CK_CK_TEMP.HCLB
        LEFT JOIN T_XT_BZLB ON T_XT_BZLB.ID=T_CK_CK_TEMP.BBZ
        where 1=1 and T_CK_CK_TEMP.SL>0</sql>
