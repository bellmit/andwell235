      <tableName>T_CK_CK</tableName>
      <sql>SELECT T_CK_CK.DWID,T_CK_CK.JX,T_CK_CK.JH,T_CK_CK.XH,
        T_CK_CK.SL,T_CK_CK.ZL,T_CK_CK.DJ,T_CK_CK.ZJ,TO_CHAR(T_CK_CK.CCRQ,&apos;YYYY-MM-DD&apos;) ccrq,
        TO_CHAR(T_CK_CK.YSRQ,&apos;YYYY-MM-DD&apos;) YSRQ,T_CK_CK.SMZT,T_CK_CK.ZSMN,T_CK_CK.ZSMXS,
        T_CK_CK.ZSMXH,T_CK_CK.ZSMQL,T_CK_CK.ZFXCS,TO_CHAR(T_CK_CK.YFRQ,&apos;YYYY-MM-DD&apos;) YFRQ,
        T_CK_CK.ZYFCS,T_CK_CK.YFCS,T_CK_CK.SBQXN,T_CK_CK.SBQXXS,T_CK_CK.BZ,T_CK_CK.ZFXXS,T_CK_CK.BCZFXXS,
        T_CK_CK.BCFXXS,T_CK_CK.ZXHCS,T_CK_CK.BCZXHCS,T_CK_CK.BCXHCS,T_CK_CK.ZQLCS,T_CK_CK.BCZQLCS,T_CK_CK.BCQLCS,
        T_CK_CK.XZ,T_CK_CK.DW,T_CK_CK.JWH,T_CK_CK.GLFS,T_CK_CK.CKDM,T_CK_CK.HL,T_CK_CK.BBZ,T_CK_CK.FJH,T_CK_CK.SFRLQ,
        T_CK_CK.ZRLQ,T_CK_CK.ZFANXCS,T_CK_CK.FANXCS,T_CK_CK.XFRQ,T_CK_CK.SFQSM,T_CK_CK.SFQSYSM,T_CK_CK.HCLB,T_CK_CK.JJ,
        T_CK_CK.ZYLB,T_CK_CK.ZGQCS,T_CK_CK.GQCS,T_CK_CK.SFQXS,T_CK_CK.YFQX,T_CK_CK.JXQN,T_CK_CK.JXQXS,T_CK_CK.RFID,T_CK_CK.YSID,
        T_CK_CK.QCLB,T_CK_CK.zqlcs yzqlcs,T_CK_CK.bczqlcs ybczqlcs
        ,TO_CHAR(ADD_MONTHS(YFRQ,12*YFQX),&apos;YYYY-MM-DD&apos;)YFQZ,T_JH_JH.MC JHMC,T_JH_JX.MC JXMC,T_XT_JLDW.MC JLDW,T_JH_ZL.MC ZLMC,T_XT_BZLB.MC BZMC,T_XT_HCLB.MC HCMC
        FROM T_CK_CK
        left join T_JH_JH ON T_JH_JH.ID=T_CK_CK.JH
        LEFT JOIN T_JH_JX ON T_JH_JX.ID=T_CK_CK.JX
        LEFT JOIN T_XT_JLDW ON T_XT_JLDW.ID=T_JH_JH.DW
        LEFT JOIN T_JH_ZL ON T_JH_ZL.ID=T_CK_CK.ZL
        LEFT JOIN T_XT_BZLB ON T_XT_BZLB.ID=T_CK_CK.BBZ
        LEFT JOIN T_XT_HCLB ON T_XT_HCLB.ID=T_CK_CK.HCLB
        WHERE 1=1 AND T_CK_CK.SL&gt;0 ORDER BY T_CK_CK.JX,JH</sql>
