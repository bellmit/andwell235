      <caption>机关调拨清单</caption>
      <tableName>detail</tableName>
      <sql>select dbdh,sqdh,jx,t_jh_jx.mc jxmc,jh,A.jhmc,A.jldw,jsdw,t_xt_dw.mc jsdwmc,B.ffdwmc,ffdw,t_db_sjsqqd.sl,zl
    from t_db_sjsqqd
    left join t_jh_jx on t_jh_jx.id=t_db_sjsqqd.jx
    left join t_xt_dw on t_xt_dw.id=t_db_sjsqqd.jsdw
    left join (select T_JH_JH.Id,T_XT_JLDW.mc jldw,T_JH_JH.mc jhmc
    from T_JH_JH
    left join T_XT_JLDW on T_JH_JH.DW = T_XT_JLDW.ID) A on t_db_sjsqqd.jh=A.id
    left join (select t_xt_dw.ID,t_xt_dw.mc ffdwmc from t_xt_dw) B on t_db_sjsqqd.ffdw=B.id</sql>
    </tableInfo>
