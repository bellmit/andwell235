    <table>
        <tableInfo>
            <caption>库存综合查询</caption>
            <tableName>T_CK_CK</tableName>
            <sql>select T_JH_JX.MC JX,JH,T_JH.MC,XH,T_CK_CK.SL,to_char(CCRQ,'YYYY-MM-DD')
                CCRQ,to_char(YSRQ,'YYYY-MM-DD') YSRQ,ZSMXS-nvl(ZFXXS,0) SYXS,
                to_char(RKRQ,'YYYY-MM-DD')RKRQ,
                to_char(YFRQ,'YYYY-MM-DD')YFRQ,TO_Char(ADD_MONTHS(YFRQ,12*YFQX),'YYYY-MM-DD')YFQZ, T_JH_ZL.mc
                ZL,T_xt_CK.MC CKDM
                from T_CK_CK
                left join T_JH on T_JH.ID=JH AND T_JH.JX=T_CK_CK.JX
                left join T_JH_JX on T_JH_JX.id=T_CK_CK.jx
                left join T_JH_ZL on T_JH_ZL.id=zl
                left join T_xt_CK on T_xt_CK.CKID=T_CK_CK.CKDM AND T_xt_CK.Dwid=T_CK_CK.DWID
                where 1=1
            </sql>
        </tableInfo>
