  <table>
    <tableInfo>
      <caption>待修库库存查询</caption>
      <tableName>T_CK_DX</tableName>
      <sql>
        SELECT a.bh ,d.mc as jhmc,A.jh,a.xh ,j.mc jldw,A.jx,c.mc jxmc,t_Jh_Zl.mc zlmc,A.zl,TO_CHAR(a.rkrq,'yyyy-mm-dd') rkrq,
        TO_CHAR(a.ccrq,'yyyy-mm-dd')ccrq ,a.zhsm as sm ,TO_CHAR(a.zhrq,'yyyy-mm-dd') zhrq,
        a.xz,a.sl,a.dwid
        FROM T_CK_DX A
        LEFT JOIN T_JH_JX C ON A.JX=C.ID
        LEFT JOIN T_JH_JH D ON D.ID = A.JH
        left join t_Jh_Zl on T_JH_ZL.id =A.ZL
        LEFT JOIN T_XT_JLDW j ON j.id=D.dw
        WHERE 1=1</sql>
    </tableInfo>
