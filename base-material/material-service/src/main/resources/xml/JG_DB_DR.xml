      <caption>机关导入展示临时表详细信息</caption>
      <tableName>detail</tableName>
      <sql>select dbdh,sqdh,jx,t_jh_jx.mc jxmc,jh,A.jhmc,A.jldw,jsdw,t_xt_dw.mc jsdwmc,B.ffdwmc,ffdw,t_db_sjsqqd_temp.sl,zl,dj,rq
    from t_db_sjsqqd_temp
    left join t_jh_jx on t_jh_jx.id=t_db_sjsqqd_temp.jx
    left join t_xt_dw on t_xt_dw.id=t_db_sjsqqd_temp.jsdw
    left join (select T_JH_JH.Id,T_XT_JLDW.mc jldw,T_JH_JH.mc jhmc
    from T_JH_JH
    left join T_XT_JLDW on T_JH_JH.DW = T_XT_JLDW.ID) A on t_db_sjsqqd_temp.jh=A.id
    left join (select t_xt_dw.ID,t_xt_dw.mc ffdwmc from t_xt_dw) B on t_db_sjsqqd_temp.ffdw=B.id</sql>
