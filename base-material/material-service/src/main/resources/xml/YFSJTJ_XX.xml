      <tableName>T_CK_YFK</tableName>
      <sql>select ROWNUM,BH,DWID,T_CK_YFK.JX,T_JH_JX.MC JXMC,JH,XH,T_CK_YFK.SL,ZL,DJ,ZJ,to_char(CCRQ,&apos;yyyy-mm-dd&apos;) CCRQ,to_char(YFRQ,&apos;yyyy-mm-dd&apos;)YFRQ,YFQX,T_XT_JLDW.MC JLDW,T_JH.MC JHMC,JWH,CKDM,SYYL,FCFF,T_XT_FCFF.MC FCMC,T_JH_ZL.MC ZLMC,xz,to_char(rkrq,&apos;yyyy-mm-dd&apos;) rkrq
        from T_CK_YFK
        LEFT JOIN T_JH ON T_JH.ID=T_CK_YFK.JH AND T_JH.JX=T_CK_YFK.JX
        LEFT JOIN T_XT_JLDW ON T_XT_JLDW.ID=T_JH.DW
        LEFT JOIN T_JH_JX ON T_JH_JX.ID=T_CK_YFK.JX
        LEFT JOIN T_JH_ZL ON T_JH_ZL.ID=T_CK_YFK.ZL
        LEFT JOIN T_XT_FCFF ON T_CK_YFK.FCFF=T_XT_FCFF.ID WHERE 1=1 AND XZ=1</sql>
