      <caption>二维码标签打印</caption>
      <tableName>CX_CXKC</tableName>
      <sql>select &apos;0&apos;XZ,T_JH_JX.MC JX,T_JH_JX.ID FJJH,JH,T_JH.MC,XH,T_CK_CK.SL,T_XT_JLDW.MC DW,to_char(CCRQ,&apos;YYYY-MM-DD&apos;) CCRQ,to_char(YSRQ,&apos;YYYY-MM-DD&apos;) YSRQ,RKRQ,to_char(XFRQ,&apos;YYYY-MM-DD&apos;) XFRQ,to_char(YFRQ,&apos;YYYY-MM-DD&apos;)YFRQ,TO_Char(ADD_MONTHS(YFRQ,12*YFQX),&apos;YYYY-MM-DD&apos;)YFQZ, T_JH_ZL.mc ZL,T_QX_DW.JM CKDM,hclb,T_XT_HCLB.MC HCLBMC,T_CK_CK. DJ,ZJ,T_CK_CK.HL,BBZ,T_XT_BZLB.MC BZMC,T_CK_CK.BZ
from T_CK_CK 
left join T_JH on T_JH.ID=JH  AND T_JH.JX=T_CK_CK.JX
left join T_JH_JX on T_JH_JX.id=T_CK_CK.jx 
left join T_JH_ZL on T_JH_ZL.id=zl 
left join T_QX_DW on T_QX_DW.ID=CKDM
left join T_XT_HCLB ON T_XT_HCLB.ID=T_CK_CK.HCLB
LEFT JOIN T_XT_BZLB ON T_XT_BZLB.ID=T_CK_CK.BBZ
left join T_XT_JLDW  on t_jh.dw = t_xt_jldw.id
where 1=1  AND T_CK_CK.SL&gt;0</sql>
