        from T_CK_CRKQD
        left join T_JH_JX on T_JH_JX.ID=T_CK_CRKQD.Jx
        left join T_JH on T_JH.id=T_CK_CRKQD.jh AND T_JH.JX=T_CK_CRKQD.JX
