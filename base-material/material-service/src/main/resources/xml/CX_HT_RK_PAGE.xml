      <tableName>MASTER</tableName>
      <sql>
        select BH, DWID,T_CK_CRK.FJLX as jx, T_CK_CRK.YJ, WH, YJRQ, T_CK_CRK.LB, RQ, FJLX, FJJH, FPH, YSFS, FFDW, FCR, JSDW, SRR, JSRQ,
        YSDH, ZXS, ZXBH, GZ, ZLY, FKZR, PZR, BGY, YSR, CJR, CJRQ, XGR, XGRQ, FFDWMC, JSDWMC,
        T_JH_JX.MC as jxmc,
        CYDW, HYDH, ZBBH, RFID, YSID, HZSJ, HZJSR, HZZT, QCLB, CRKLX, JFRQ ,  A.Mc as ffmc,b.mc as jsmc
        from T_CK_CRK
        left join V_DW A on A.id=T_CK_CRK.Ffdw
        left join V_DW B on B.id=T_CK_CRK.Jsdw
        left join T_JH_JX on T_JH_JX.ID=T_CK_CRK.FJLX
        where 1=1 order by RQ desc
      </sql>
    </tableInfo>
