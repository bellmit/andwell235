      <sql>SELECT A.BH,PCH,A.LB,A.HCLB,C.MC HCLBMC,D.MC JLDWMC,A.RQ,JWH,A.MC,GG,DW,DJ,SL,DJ*SL ZJ,JYR,JHR,YXQ,JSR,LYR,KGY,BZ,CKDM,E.MC CKMC,SYDW,A.LYLB,F.MC LYLBMC FROM T_XHP_CRKQD A     LEFT JOIN T_XHP_LB C ON A.HCLB=C.ID     LEFT JOIN T_XT_JLDW D ON A.DW=D.ID     LEFT JOIN T_XT_CK E ON A.CKDM=E.CKID  AND A.DWID=E.DWID     LEFT JOIN T_CK_CRKLB F ON A.LYLB=F.ID     WHERE 1=1</sql>
