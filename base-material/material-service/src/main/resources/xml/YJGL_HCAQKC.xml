<tableList describe="航材安全库存量设置" xmlName="YJGL_HCAQKC">
  <table>
    <tableInfo>
      <caption>航材安全库存量设置</caption>
      <tableName>T_CK_AQKC</tableName>
      <sql>select B.jh,B.JX,T_JH_JX.MC AS JXMC,
        T_JH_JH.MC JHMC,B.aqkc,B.gsd,B.dcd
        FROM T_CK_AQKC B
        LEFT JOIN T_JH ON B.JH=T_JH.ID and B.JX=T_JH.JX
        LEFT JOIN T_JH_JX ON B.JX=T_JH_JX.ID
        LEFT JOIN T_JH_JH ON B.JH=T_JH_JH.ID
        where 1=1
        group by B.jh,B.JX,B.aqkc,B.gsd,B.dcd,T_JH_JX.MC,T_JH_JH.MC
        order by B.JX
      </sql>
    </tableInfo>
