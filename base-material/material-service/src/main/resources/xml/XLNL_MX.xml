<tableList describe="修理能力明细" xmlName="XLNL_MX">
    <table>
        <tableInfo>
            <caption>修理能力明细</caption>
            <tableName>T_CK_SX</tableName>
            <sql>
            select
            tJX.Mc jxmc,t.jh,tJH.Mc jhmc,v.MC dwmc,round(avg(t.clsj-t.sxrq),2) avgTime,round(max(t.clsj-t.sxrq),2) maxTime
            from T_CK_SX t
            join V_DW v on t.Dwid = v.ID
            join T_JH_JH tJH on tJH.Id = t.jh
            join T_JH_JX tJX on tJX.Id = t.jx
            group by tJX.Mc,t.jh,tJH.Mc,v.MC</sql>
        </tableInfo>
