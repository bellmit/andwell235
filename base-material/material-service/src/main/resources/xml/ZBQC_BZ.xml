<tableList describe="战备器材标准" xmlName="ZBQC_BZ">
  <table>
    <tableInfo>
      <caption>战备器材标准</caption>
      <tableName>MASTER</tableName>
      <sql>SELECT
        A.*,B.MC JXMC,e.mc jhmc,f.mc jldw
        FROM T_ZB_BZ A
        LEFT JOIN T_JH_JX B ON  A.JX=B.ID
        LEFT JOIN T_JH C ON A.JH=C.ID AND A.JX=C.JX
        LEFT JOIN T_XT_JLDW D ON C.DW=D.ID
        left join t_jh_jh E on a.jh=e.id
        left join T_XT_JLDW F on e.dw=f.id

        WHERE 1=1 ORDER BY A.JX,JH     </sql>
    </tableInfo>
