  <table>
    <tableInfo>
      <caption>日历期预警</caption>
      <tableName>T_CK_CK</tableName>
      <sql>SELECT B.jx,B.JXMC,B.JH,B.JHMC,B.JLDW,B.XH,B.SL,b.ZL,B.ZLMC,B.DJ,B.ZJ,B.DW,B.JWH,B.CKDM,B.BBZ,
b.DWID,B.HCLBMC,B.CCRQ,B.FXRQ,B.ZRLQ,B.SFRLQ,B.YFRQ,B.YFQX,B.XFRQ,B.RLQZ,B.YFQZ,
CASE WHEN  RLQZ&gt;SJRLQZ THEN &apos;0&apos; WHEN RLQZ&lt;SJRLQZ AND RLQZ&gt;EJRLQZ THEN &apos;3&apos; WHEN RLQZ&lt;EJRLQZ AND RLQZ&gt;YJRLQZ THEN &apos;2&apos; WHEN RLQZ&lt;YJRLQZ THEN &apos;1&apos; ELSE &apos;4&apos; END ZT
  FROM (
        SELECT A.JX,T_JH_JX.MC JXMC,JH,T_JH.MC JHMC,T_XT_JLDW.MC JLDW,XH,A.SL SL,ZL,T_JH_ZL.MC ZLMC,DJ,ZJ,A.DW,JWH,CKDM,HCLB,BBZ,A.DWID,
        T_XT_HCLB.MC HCLBMC,CCRQ,FXRQ,ZRLQ,SFRLQ,YFRQ,YFQX,XFRQ,case when XFRQ IS null then add_months(CCRQ,ZRLQ*12)  WHEN XFRQ IS NOT null  then add_months(XFRQ,ZRLQ*12)
        END RLQZ,ADD_MONTHS(YFRQ,12*YFQX) YFQZ,
        YJRL,EJRL,SJRL,SYSDATE+YJRL YJRLQZ,SYSDATE+EJRL EJRLQZ,SYSDATE+SJRL SJRLQZ
        FROM T_CK_CK A
        LEFT JOIN T_JH_JX ON T_JH_JX.ID=A.JX
        LEFT JOIN T_JH ON T_JH.ID=A.JH AND T_JH.JX=A.JX
        LEFT JOIN T_XT_JLDW ON T_JH.DW=T_XT_JLDW.ID
        LEFT JOIN T_XT_HCLB ON T_XT_HCLB.ID=A.HCLB
        LEFT JOIN T_JH_ZL ON T_JH_ZL.ID=A.ZL
        LEFT JOIN  T_XT_HCYJ  on 1=1
        WHERE   ZRLQ IS NOT NULL AND A.SL&gt;0 AND ZRLQ&gt;0 
)B WHERE 1=1
        
      </sql>
    </tableInfo>
