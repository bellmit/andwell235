  <table>
    <tableInfo>
      <caption>五金器材库存查询</caption>
      <tableName>MASTER</tableName>
      <sql>SELECT  A.MC,A.GG,A.PCH,A.DW,C.MC JLDW,A.DJ,A.SL,A.DJ*A.SL ZJ,A.LB,B.MC LBMC,A.JWH,A.CKDM,D.MC CKMC,A.DWID,RKRQ,A.YXQ,LYDW,A.LYLB,E.MC LYLBMC,F.JYR,F.JHR,F.KGY,F.TZS
FROM T_XHP_CK A       
LEFT JOIN T_XHP_LB B ON A.LB=B.ID         
LEFT JOIN T_XT_CK D ON A.CKDM=D.CKID AND A.DWID=D.DWID       
LEFT JOIN T_XT_JLDW C ON A.DW=C.ID        
LEFT JOIN T_CK_CRKLB E ON A.LYLB=E.ID  
LEFT JOIN T_XHP_CRKQD F ON A.PCH=F.PCH AND A.MC=F.MC AND A.GG=F.GG
WHERE 1=1 AND A.SL&gt;0 order by A.PCH desc </sql>
    </tableInfo>
