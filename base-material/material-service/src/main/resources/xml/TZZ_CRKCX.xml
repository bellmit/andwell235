      <tableName>MASTER</tableName>
      <sql> SELECT A.BH,FFDW,JSDW,A.RQ,WH,YJ,FFR,JSR,YSFS,A.CRKLB,B.JH,D.MC,XH,C.MC JLDWMC,SL,DJ,ZJ,CCRQ,D.LB WZLBID,F.MC WZLB,CJH,FDJH,CKDM,E.MC CKMC,JWH,BZ       FROM T_TZZ_CRK A            LEFT JOIN T_TZZ_CRKQD B ON A.BH=B.BH           LEFT JOIN T_TZZ_WZ D ON B.JH=D.JH  LEFT JOIN T_TZZ_WZLB F ON F.ID=D.LB    LEFT JOIN T_XT_JLDW C ON D.DW=C.ID      LEFT JOIN T_XT_CK E ON B.CKDM=E.CKID AND B.DWID=E.DWID      WHERE 1=1 ORDER BY BH,B.JH</sql>
