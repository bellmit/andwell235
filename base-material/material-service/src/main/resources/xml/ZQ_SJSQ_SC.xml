      <tableName>T_DB_SQSJ</tableName>
      <sql>select T_DB_SJSQ.*,v1.mc sqdwmc,T_JH_JX.mc jxmc,
CASE WHEN clzt=&apos;0&apos; THEN &apos;未处理&apos; WHEN clzt=&apos;1&apos; THEN &apos;已处理&apos; ELSE &apos;未知&apos; END clztmc
