      <tableName>T_JH</tableName>
      <sql>SELECT T_JH.ID JH,T_JH.MC JHmc,T_JH.MC,T_XT_JLDW.MC DW,T_JH_JX.MC JX,T_JH.JX SJX,ZYLX,T_JH_ZYLX.MC LXMC,t_JH_jx.Id id FROM T_JH
        LEFT JOIN t_JH_jx ON t_JH_jx.ID=T_JH.JX
