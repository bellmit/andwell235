
<tableList  describe="待修件信息查询" xmlName="KFGL_DXJXX">
<table>
<tableInfo>
  <caption>待修件信息查询</caption>
  <tableName>t_ck_dx</tableName>
  <sql>select CKDM,T_XT_CK.MC CKMC,JH,T_JH.MC JHMC,T_XT_JLDW.MC JLDWMC,T_JH_JX.MC JXMC,XH,A.JX,T_JH_ZL.MC ZLMC,RKRQ,SYDW,GZRQ,GZXX,BZ,BCXHCS,BCFXXS
,A.SL,XZ,CX.MC CXDWMC,YJ,A.DWID,ZSMN,ZSMXS,ZSMQL,YFRQ,YFQX,HCLB,T_XT_HCLB.MC HCMC,ZY
from t_ck_dx A
left join T_JH ON T_JH.ID=A.JH AND T_JH.JX=A.JX
LEFT JOIN T_XT_JLDW ON T_XT_JLDW.ID=T_JH.DW
LEFT JOIN T_JH_JX ON T_JH_JX.ID=A.JX
LEFT JOIN T_JH_ZL ON T_JH_ZL.ID=A.ZL
LEFT JOIN T_XT_HCLB ON T_XT_HCLB.ID=A.HCLB
LEFT JOIN V_DW CX ON CX.ID=A.CXDW
LEFT JOIN T_XT_CK ON T_XT_CK.DWID=A.DWID AND T_xt_ck.Ckid=A.CKDM
WHERE 1=1 AND XZ='0'</sql>
</tableInfo>
