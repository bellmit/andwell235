LEFT JOIN T_XT_CK ON T_XT_CK.DWID=A.DWID AND T_xt_ck.Ckid=A.CKDM
LEFT JOIN T_XT_BZLB ON T_XT_BZLB.ID=A.BZZ
LEFT JOIN t_xt_xljb ON t_xt_xljb.id=A.XLJB</sql>
