<tableInfo>
  <caption>送修信息查询</caption>
  <tableName>T_CK_SX</tableName>
  <sql>SELECT JH,T_JH.MC JHMC,T_XT_JLDW.MC JLDWMC,T_JH_JX.MC JXMC,XH,FJH,BZ,T_JH_ZL.MC ZLMC,A.JX,CKDM,T_XT_CK.MC CKMC,FHD,CX.MC CXDWMC,CXDW,SXRQ,SXLB,XZ,A.SL,HCLB,T_XT_HCLB.MC HCMC,GZYY,JXQN,JXQXS 
FROM T_CK_SX A
left join T_JH ON T_JH.ID=A.JH AND T_JH.JX=A.JX
LEFT JOIN T_XT_JLDW ON T_XT_JLDW.ID=T_JH.DW
LEFT JOIN T_JH_JX ON T_JH_JX.ID=A.JX
LEFT JOIN T_JH_ZL ON T_JH_ZL.ID=A.ZL
LEFT JOIN T_XT_HCLB ON T_XT_HCLB.ID=A.HCLB
LEFT JOIN V_DW CX ON CX.ID=A.CXDW
LEFT JOIN T_XT_CK ON T_XT_CK.DWID=A.DWID AND T_xt_ck.Ckid=A.CKDM
WHERE 1=1 AND A.SL>0 AND XZ=0</sql>
