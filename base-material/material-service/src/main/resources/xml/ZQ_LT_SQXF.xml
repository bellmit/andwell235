LEFT JOIN V_DW v1 ON T_DB_LTSQ.sqdw = v1.id
LEFT JOIN V_DW v2 ON T_DB_LTSQ.JSDW = v2.id
LEFT JOIN T_JH_JX ON T_JH_JX.id = T_DB_LTSQ.jx
