      <tableName>T_CK_YFK</tableName>
      <sql>select BH,DWID,T_CK_YFK.JX,T_JH_JX.MC JXMC,JH,XH,T_CK_YFK.SL,ZL,DJ,ZJ,CCRQ,YFRQ,YFQX,T_XT_JLDW.MC JLDW,T_JH.MC JHMC,JWH,CKDM,SYYL,FCFF,T_XT_FCFF.MC FCMC,T_XT_FCFF.JM as FCJC,T_JH_ZL.MC ZLMC,BZ,TO_Char(ADD_MONTHS(YFRQ,12*YFQX),&apos;YYYY-MM-DD&apos;)YFQZ
                from T_CK_YFK LEFT JOIN T_JH ON T_JH.ID=T_CK_YFK.JH AND T_JH.JX=T_CK_YFK.JX
                LEFT JOIN T_XT_JLDW ON T_XT_JLDW.ID=T_JH.DW
                LEFT JOIN T_JH_JX ON T_JH_JX.ID=T_CK_YFK.JX
                LEFT JOIN T_JH_ZL ON T_JH_ZL.ID=T_CK_YFK.ZL
                LEFT JOIN T_XT_FCFF ON T_CK_YFK.FCFF=T_XT_FCFF.ID WHERE 1=1 AND XZ&gt;1</sql>
