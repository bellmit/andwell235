  <table>
    <tableInfo>
      <caption>用户管理查询</caption>
      <tableName>T_QX_YH</tableName>
      <sql>SELECT
        A.DLM,A.ID,A.DW DWID,A.YHM,A.ZW,T_QX_DW.MC DWMC,T_RY_ZW.MC ZWMC,T_RY_JB.MC JBMC,T_RY_JX.MC JXMC,CSRQ,RWRQ,ZZMM,RDTRQ,T_QX_JS.MC JSMC,T_RY_ZZMM.MC ZZMMMC,spqx spqxid,CASE WHEN SPQX=0 then '无' else '有'
        end SPQX
        FROM T_QX_YH A
        LEFT JOIN T_QX_DW ON A.DW=T_QX_DW.ID
        LEFT JOIN T_RY_ZW ON A.ZW=T_RY_ZW.ID
        LEFT JOIN T_RY_JB ON A.JB=T_RY_JB.ID
        LEFT JOIN T_RY_JX ON A.JX=T_RY_JX.ID
        LEFT JOIN T_RY_ZZMM ON A.ZZMM=T_RY_ZZMM.ID
        LEFT JOIN T_QX_JS ON A.JS=T_QX_JS.ID
        WHERE 1=1  ORDER BY ID</sql>
