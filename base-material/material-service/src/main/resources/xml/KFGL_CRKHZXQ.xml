  <table>
    <tableInfo>
      <caption>库房管理</caption>
      <tableName>T_GZ_HZ</tableName>
      <sql>SELECT A.LB,D.id,D.MC LBMC,A.JX,B.MC JXMC,A.JH,<C class=""></C>MC JHMC,E.MC JLDW,A.SL,A.ZJ FROM (
        SELECT LB,JX, JH, SUM(SL) SL, sum(dj * sl * nvl(hl, 1)) zj   FROM T_CK_CRKQD A WHERE 1=1  AND CXSGIN = 0 GROUP BY JX, JH, LB
        )A
        LEFT JOIN T_JH_JX B ON B.ID=A.JX
        LEFT JOIN T_JH C ON A.JH=C.ID AND A.JX=C.JX
        LEFT JOIN T_XT_JLDW E ON C.DW=E.ID
        LEFT JOIN T_CK_CRKLB D ON D.ID=A.LB
        order by A.LB, A.JX  </sql>
    </tableInfo>
