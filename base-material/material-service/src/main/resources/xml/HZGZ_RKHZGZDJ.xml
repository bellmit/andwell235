  <table>
    <tableInfo>
      <caption>入库回执跟踪登记</caption>
      <tableName>T_CK_CRK</tableName>
      <sql>select T_CK_CRK.BH,T_CK_CRK.Yj,T_CK_CRK.Wh,T_CK_CRK.Ffdw ffmc,T_CK_CRK.Jsdw jsmc,T_CK_CRK.lb,T_CK_CRKLB.MC CRKLB
        ,A.Mc as FFdw,b.mc as JSdw, to_char(T_CK_CRK.RQ,'YYYY-MM-DD')RQ,T_JH_JX.mc as JXMC,T_JH_JX.ID JX,T_CK_FYFS.Mc as FYFSMC
        from T_CK_CRK
        left join (SELECT ID,MC FROM T_XT_DW
        UNION
        SELECT ID,MC FROM T_CF) A on A.id=T_CK_CRK.Ffdw
        left join V_DW B on B.id=T_CK_CRK.Jsdw left join T_CK_CRKLB ON T_CK_CRKLB.ID=T_CK_CRK.LB
        left join T_JH_JX on T_CK_CRK.Fjlx=T_JH_JX.Id
        left join T_CK_FYFS on T_CK_FYFS.ID=T_CK_CRK.YSFS
        where 1=1 and T_CK_CRK.bh not in (select bh from t_gz_HZ group by bh)
        and T_CK_CRK.lb like '10%'order by T_CK_CRK.RQ DESC</sql>
    </tableInfo>
