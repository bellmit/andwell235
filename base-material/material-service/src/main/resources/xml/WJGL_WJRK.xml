  <table>
    <tableInfo>
      <caption>五金入库</caption>
      <tableName>DETAIL</tableName>
      <sql> SELECT A.BH,A.PCH,FFDW,SRDW,A.LB,B.HCLB,C.MC HCLBMC,D.MC JLDWMC,A.RQ,JWH,B.MC,GG,DW,DJ,TZS,SL,DJ*SL ZJ,JYR,JHR,YXQ,JSR,KGY,LYR,BZ,CKDM,E.MC CKMC,SYDW,LYLB,F.MC LYLBMC
       FROM T_XHP_CRK A
      LEFT JOIN T_XHP_CRKQD B ON A.BH=B.BH
      LEFT JOIN T_XHP_LB C ON B.HCLB=C.ID
      LEFT JOIN T_XT_JLDW D ON B.DW=D.ID
      LEFT JOIN T_XT_CK E ON B.CKDM=E.CKID
          LEFT JOIN T_CK_CRKLB F ON B.LYLB=F.ID
            WHERE 1=1 AND SL>0</sql>
