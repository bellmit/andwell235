      <caption>五金入库</caption>
      <tableName>MASTER</tableName>
      <sql>SELECT A.MC,GG,PCH,DW,C.MC JLDW,A.DJ,a.SL,B.MC LBMC,D.MC CKMC,A.DWID,JWH,RKRQ,YXQ,A.LYDW,LYLB,E.MC LYLBMC
        FROM T_XHP_CK A
        LEFT JOIN T_XHP_LB B ON to_number(A.LB)=B.ID
        LEFT JOIN T_XT_CK D ON A.CKDM=D.CKID AND A.DWID=D.DWID
        LEFT JOIN T_XT_JLDW C ON A.DW=C.ID
        LEFT JOIN T_CK_CRKLB E ON A.LYLB=E.ID
        WHERE 1=1 AND A.SL>0 order by pch desc</sql>
