      <caption>油封数据确认查询</caption>
      <tableName>T_CK_YFK</tableName>
      <sql>select ROWNUM,BH,DWID,T_CK_YFK.JX,T_JH_JX.MC JXMC,JH,XH,T_CK_YFK.SL,ZL,DJ,ZJ,TO_CHAR(CCRQ,&apos;yyyy-mm-dd&apos;) CCRQ,TO_CHAR(YFRQ,&apos;yyyy-mm-dd&apos;) YFRQ,YFQX,T_XT_JLDW.MC JLDW,T_JH.MC JHMC,JWH,CKDM,T_JH_ZL.MC ZLMC,
        TO_CHAR(RKRQ,&apos;yyyy-mm-dd&apos;) rkrq,ADD_MONTHS(YFRQ,YFQX*12) YFQZ
        from T_CK_YFK LEFT JOIN T_JH ON T_JH.ID=T_CK_YFK.JH  AND T_JH.JX=T_CK_YFK.JX
        LEFT JOIN T_XT_JLDW ON T_XT_JLDW.ID=T_JH.DW
        LEFT JOIN T_JH_JX ON T_JH_JX.ID=T_CK_YFK.JX
        LEFT JOIN T_JH_ZL ON T_JH_ZL.ID=T_CK_YFK.ZL WHERE 1=1 AND XZ=0</sql>
