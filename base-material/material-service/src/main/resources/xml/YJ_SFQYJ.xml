  <table>
    <tableInfo>
      <caption>首翻期预警</caption>
      <tableName>T_CK_CK</tableName>
      <sql>SELECT A.JX,A.JXMC,A.JH,A.JHMC,A.JLDW,A.XH,A.SL,A.ZL,A.ZLMC,A.DJ,A.ZJ,JWH,A.CKDM,A.HCLB,A.BBZ,
A.HCLBMC,A.CCRQ,A.SFRLQ,A.FXRQ,A.XFRQ,A.SFRLQZ,A.DWID,A.qclbmc 
,YJSF,EJSF,SJSF,SYSDATE+YJSF YJSFQZ, SYSDATE+EJSF EJSFQZ,SYSDATE+SJSF SJSFQZ
,CASE WHEN  SFRLQZ&gt;SJSFQZ THEN &apos;0&apos; WHEN SFRLQZ&lt;SJSFQZ AND SFRLQZ&gt;EJSFQZ THEN &apos;3&apos; WHEN SFRLQZ&lt;EJSFQZ AND SFRLQZ&gt;YJSFQZ THEN &apos;2&apos; WHEN SFRLQZ&lt;YJSFQZ THEN &apos;1&apos; ELSE &apos;4&apos; END ZT
 FROM (
SELECT T_CK_CK.JX,T_JH_JX.MC JXMC,T_CK_CK.JH,T_JH.MC JHMC,T_CK_CK.DW JLDW,T_CK_CK.XH,T_CK_CK.SL,T_CK_CK.ZL,T_JH_ZL.MC ZLMC,T_CK_CK.DJ,T_CK_CK.ZJ,JWH,T_CK_CK.CKDM,T_CK_CK.HCLB,T_CK_CK.BBZ,
T_XT_HCLB.MC HCLBMC,T_CK_CK.CCRQ,T_CK_CK.SFRLQ,T_CK_CK.FXRQ,T_CK_CK.XFRQ
,ADD_MONTHS(CCRQ,12*sfrlq) SFRLQZ
,T_CK_CK.DWID,CASE WHEN T_CK_CK.QCLB=1 THEN &apos;周转航材&apos; WHEN T_CK_CK.QCLB=2 THEN &apos;战备器材&apos;else&apos;其他&apos; end qclbmc 
,YJSF,EJSF,SJSF,SYSDATE+YJSF YJSFQZ, SYSDATE+EJSF EJSFQZ,SYSDATE+SJSF SJSFQZ
FROM T_CK_CK
LEFT JOIN T_JH_JX ON T_JH_JX.ID=T_CK_CK.JX
LEFT JOIN T_JH ON T_JH.ID=T_CK_CK.JH AND T_JH.JX=T_CK_CK.JX
LEFT JOIN T_XT_JLDW ON T_JH.DW=T_XT_JLDW.MC
LEFT JOIN T_XT_HCLB ON T_XT_HCLB.ID=T_CK_CK.HCLB
LEFT JOIN T_JH_ZL ON T_JH_ZL.ID=T_CK_CK.ZL  
LEFT JOIN  T_XT_HCYJ  on 1=1
WHERE 1=1 AND T_CK_CK.SL&gt;0  
)A </sql>
    </tableInfo>
