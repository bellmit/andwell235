
<tableList describe="旅团合同入库单上报" xmlName = "SJGL_LTXFJGHT">
  <table>
    <tableInfo>
      <caption>旅团合同入库单上报</caption>
      <tableName>RKXQ1</tableName>
      <sql>select
        T_CK_CRK.*,T_CK_CRK.Yj HTBH ,A.Mc as FFMC,b.mc as JSMC, T_JH_JX.Id as JX,T_JH_JX.mc as JXMC,T_CK_FYFS.Mc as FYFSMC,c.MC CRKLXMC
        from
        T_CK_CRK
        left join V_DW A on A.id=T_CK_CRK.Ffdw
        left join V_DW B on B.id=T_CK_CRK.Jsdw
        left join T_JH_JX on T_CK_CRK.Fjlx=T_JH_JX.Id
        left join T_CK_FYFS on T_CK_FYFS.ID=T_CK_CRK.YSFS
        left join T_CK_CRKLX C on T_CK_CRK.CRKLX=C.id
        where 1=1 and T_CK_CRK.Yj is not null and T_CK_CRK.Bh like 'HTRK%' OR T_CK_CRK.Bh like 'HTDDRK%' order by T_CK_CRK.WH</sql>
    </tableInfo>
