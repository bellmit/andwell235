<tableList describe="合同入库单查询" xmlName="T_HT_RKD">
  <table>
    <tableInfo>
      <caption>合同入库单查询</caption>
      <tableName>MASTER</tableName>
      <sql>select A.YJ,T_HT.CJID,T_HT.JHBH,A.BH as RKDH, A.WH as HTWH,T_CF.MC as CJMC,A.RQ as RKRQ,HT_FK_QD.FKBH,T_HT_FK.FKRQ,T_HT.STATUS, B.RKXS,B.RKJS,B.ZJ

from T_CK_CRK A

LEFT JOIN T_HT_FK ON T_HT_FK.HTBH=A.YJ

LEFT JOIN T_HT ON T_HT.HTH=A.YJ
LEFT JOIN HT_FK_QD ON HT_FK_QD.HTH=A.YJ

LEFT JOIN T_CF ON T_CF.ID=T_HT.CJID

LEFT JOIN (select T_CK_CRK.YJ,T_CK_CRK.BH,count(T_CK_CRKQD.JH) as RKXS,sum(T_CK_CRKQD.Sl) as RKJS,sum(T_CK_CRKQD.ZJ) as ZJ

FROM T_CK_CRKQD 

LEFT JOIN T_CK_CRK ON T_CK_CRKQD.BH=T_CK_CRK.BH AND T_CK_CRKQD.DWID=T_CK_CRK.DWID

GROUP BY T_CK_CRK.YJ,T_CK_CRK.BH) B ON A.YJ=B.YJ</sql>
    </tableInfo>
