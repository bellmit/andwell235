      <sql>
                select T_DH_JHQD.BH,
                T_DH_JHQD.JX,
                T_DH_JHQD.JH,
                T_DH_JHQD.QCLX,
                T_DH_JHQD.SQSL,
                T_CG_ML_QD.CJ,
                T_CG_ML_QD.jgdw,
                T_CG_ML_QD.DJ,
                T_CG_ML_QD.JG_YY as jgyy,
                T_CG_ML_QD.JG_LX as jglx,
                T_JH_JH.MC   as jhmc,
                T_JH_JX.MC   as jxmc,
                T_XT_JLDW.MC as jldwmc,
                T_XT_QCLX.MC as qclxmc,
                T_CG_ML.ID   as mlid,
                T_CG_ML_QD.ID as mlQdid,
                T_CF.MC      as cjmc
                from T_DH_JHQD
                LEFT JOIN T_XT_QCLX ON T_XT_QCLX.ID = T_DH_JHQD.QCLX
                LEFT JOIN T_JH_JH ON T_DH_JHQD.JH = T_JH_JH.ID
                LEFT JOIN T_XT_JLDW ON T_JH_JH.DW = T_XT_JLDW.ID
                LEFT JOIN T_JH_JX ON T_DH_JHQD.JX = T_JH_JX.ID
                LEFT JOIN T_CG_ML_QD ON T_CG_ML_QD.JH = T_DH_JHQD.JH and T_CG_ML_QD.JX = T_DH_JHQD.JX
                LEFT JOIN T_CF ON T_CF.ID = T_CG_ML_QD.CJ
                LEFT JOIN T_CG_ML ON T_CG_ML.ML_BH = T_CG_ML_QD.ML_BH
                where T_CG_ML.STATUS = 1
        </sql>
